                            </m:mrow>
                          </m:mrow>
                        </m:mstyle>
</m:mrow>
</m:munder>        
              <m:mo form="prefix">sinc</m:mo>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:mfrac>
                          <m:mi>P</m:mi>
                          <m:mi>T</m:mi>
                        </m:mfrac>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:mi>t</m:mi>
                          <m:mo>-</m:mo>
                          <m:mi>k</m:mi>
                          <m:mfrac>
                            <m:mi>T</m:mi>
