                      <m:mi>a</m:mi>
                      <m:mrow>
                        <m:mo>[</m:mo>
                        <m:mi>n</m:mi>
                        <m:mo>]</m:mo>
                      </m:mrow>
                      <m:mi>g</m:mi>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>-</m:mo>
                        <m:mi>n</m:mi>
                        <m:mi>T</m:mi>
                        <m:mo>)</m:mo>
                      </m:mrow>
                    </m:mrow>
                  </m:mstyle>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd/>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mstyle scriptlevel="0" displaystyle="false">
                    <m:mrow>
                      <m:msub>
                        <m:mo>∑</m:mo>
                        <m:mi>n</m:mi>
                      </m:msub>
                      <m:mi>a</m:mi>
                      <m:mrow>
                        <m:mo>[</m:mo>
                        <m:mi>n</m:mi>
                        <m:mo>]</m:mo>
                      </m:mrow>
                      <m:msub>
                        <m:mo>∑</m:mo>
                        <m:mi>l</m:mi>
                      </m:msub>
                      <m:mi>g</m:mi>
                      <m:mrow>
                        <m:mo>[</m:mo>
                        <m:mi>l</m:mi>
                        <m:mo>]</m:mo>
                      </m:mrow>
                      <m:mo form="prefix">sinc</m:mo>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:mfrac>
                          <m:mi>P</m:mi>
                          <m:mi>T</m:mi>
                        </m:mfrac>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:mi>t</m:mi>
                          <m:mo>-</m:mo>
                          <m:mi>n</m:mi>
                          <m:mi>T</m:mi>
                          <m:mo>-</m:mo>
                          <m:mi>l</m:mi>
                          <m:mfrac>
                            <m:mi>T</m:mi>
                            <m:mi>P</m:mi>
                          </m:mfrac>
                          <m:mo>)</m:mo>
                        </m:mrow>
                        <m:mo>)</m:mo>
                      </m:mrow>
                    </m:mrow>
                  </m:mstyle>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd/>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mstyle scriptlevel="0" displaystyle="false">
                    <m:mrow>
                      <m:msub>
                        <m:mo>∑</m:mo>
                        <m:mi>n</m:mi>
                      </m:msub>
                      <m:mi>a</m:mi>
                      <m:mrow>
                        <m:mo>[</m:mo>
                        <m:mi>n</m:mi>
                        <m:mo>]</m:mo>
                      </m:mrow>
                      <m:msub>
                        <m:mo>∑</m:mo>
                        <m:mi>k</m:mi>
                      </m:msub>
                      <m:mi>g</m:mi>
                      <m:mrow>
                        <m:mo>[</m:mo>
                        <m:mi>k</m:mi>
                        <m:mo>-</m:mo>
                        <m:mi>n</m:mi>
                        <m:mi>P</m:mi>
                        <m:mo>]</m:mo>
                      </m:mrow>
                      <m:mo form="prefix">sinc</m:mo>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:mfrac>
                          <m:mi>P</m:mi>
                          <m:mi>T</m:mi>
                        </m:mfrac>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:mi>t</m:mi>
                          <m:mo>-</m:mo>
                          <m:mi>k</m:mi>
                          <m:mfrac>
                            <m:mi>T</m:mi>
                            <m:mi>P</m:mi>
                          </m:mfrac>
                          <m:mo>)</m:mo>
                        </m:mrow>
                        <m:mo>)</m:mo>
                      </m:mrow>
                      <m:mspace width="4.pt"/>
                      <m:mtext>via</m:mtext>
                      <m:mspace width="4.pt"/>
                      <m:mrow>
                        <m:mi>k</m:mi>
                        <m:mo>=</m:mo>
                        <m:mi>n</m:mi>
                        <m:mi>P</m:mi>
                        <m:mo>-</m:mo>
                        <m:mi>l</m:mi>
                      </m:mrow>
                    </m:mrow>
                  </m:mstyle>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd/>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mstyle scriptlevel="0" displaystyle="false">
                    <m:mrow>
                      <m:msub>
                        <m:mo>∑</m:mo>
                        <m:mi>k</m:mi>
                      </m:msub>
<m:munder>
<m:mrow>                      
  <m:munder accentunder="true">
                          <m:mstyle scriptlevel="0" displaystyle="false">
                            <m:mrow>
                              <m:msub>
                                <m:mo>∑</m:mo>
                                <m:mi>n</m:mi>
                              </m:msub>
                              <m:mi>a</m:mi>
                              <m:mrow>
                                <m:mo>[</m:mo>
                                <m:mi>n</m:mi>
                                <m:mo>]</m:mo>
                              </m:mrow>
                              <m:mi>g</m:mi>
                              <m:mrow>
                                <m:mo>[</m:mo>
                                <m:mi>k</m:mi>
                                <m:mo>-</m:mo>
                                <m:mi>n</m:mi>
                                <m:mi>P</m:mi>
                                <m:mo>]</m:mo>
                              </m:mrow>
                            </m:mrow>
                          </m:mstyle>
                          <m:mo>︸</m:mo>
                        </m:munder>
</m:mrow>
<m:mrow>
<m:mstyle scriptlevel="0" displaystyle="true">
                          <m:mrow>
                            <m:mo>:=</m:mo>
                            <m:mover accent="true">
                              <m:mi>m</m:mi>
                              <m:mo>˜</m:mo>
                            </m:mover>
                            <m:mrow>
                              <m:mo>[</m:mo>
