                    <m:mrow>
                      <m:mfrac>
                        <m:mn>1</m:mn>
                        <m:mn>2</m:mn>
                      </m:mfrac>
                      <m:mfenced separators="" open="[" close="]">
                        <m:mi>M</m:mi>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:mi>f</m:mi>
                          <m:mspace width="-0.166667em"/>
                          <m:mo>-</m:mo>
                          <m:mspace width="-0.166667em"/>
                          <m:msub>
                            <m:mi>f</m:mi>
