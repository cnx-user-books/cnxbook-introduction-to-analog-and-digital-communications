                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mn>2</m:mn>
                      <m:mi>π</m:mi>
                      <m:msub>
                        <m:mi>f</m:mi>
                        <m:mi>c</m:mi>
                      </m:msub>
                      <m:mi>t</m:mi>
