                    <m:mfrac>
                      <m:mi>d</m:mi>
                      <m:mrow>
                        <m:mi>d</m:mi>
                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mi>s</m:mi>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>t</m:mi>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:mrow>
                </m:mtd>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mstyle scriptlevel="0" displaystyle="false">
                    <m:mrow>
                      <m:mfrac>
                        <m:mi>d</m:mi>
                        <m:mrow>
                          <m:mi>d</m:mi>
