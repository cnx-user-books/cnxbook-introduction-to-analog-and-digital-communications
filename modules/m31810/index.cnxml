              </m:mtr>
              <m:mtr>
                <m:mtd/>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mstyle scriptlevel="0" displaystyle="false">
                    <m:mrow>
                      <m:mo>-</m:mo>
                      <m:mfenced separators="" open="[" close="]">
                        <m:mn>2</m:mn>
                        <m:mi>π</m:mi>
